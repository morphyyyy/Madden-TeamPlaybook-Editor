<Application 
    x:Class="Madden20PlaybookEditor.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary"/>
            <Color x:Key="Secondary"/>
            <Color x:Key="Tertiary"/>
            <Color x:Key="Quaternary"/>
            <Color x:Key="Quinary"/>
            <Color x:Key="Senary"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/FormationStyle.xaml" />
                <ResourceDictionary Source="/Styles/FormationTabItemStyle.xaml" />
                <ResourceDictionary Source="/Styles/SubFormationStyle.xaml" />
                <ResourceDictionary Source="/Styles/SubFormationTabItemStyle.xaml" />
                <ResourceDictionary Source="/Styles/PlayStyle.xaml" />
                <ResourceDictionary Source="/Styles/PlayTabItemStyle.xaml" />
                <ResourceDictionary Source="/Styles/SituationStyle.xaml" />
                <ResourceDictionary Source="/Styles/AudibleDeep.xaml" />
                <ResourceDictionary Source="/Styles/AudibleFake.xaml" />
                <ResourceDictionary Source="/Styles/AudibleQuick.xaml" />
                <ResourceDictionary Source="/Styles/AudibleRun.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
                        </Style.Triggers>
                    </Style>
                </ItemsControl.ItemContainerStyle>
            </ItemsControl>
        </Canvas>

        <DockPanel 
            Margin="0,0,0,0" 
            x:Name="ReferenceInfo" 
            Canvas.Left="0" 
            Canvas.Top="0" 
            Height="800" 
            Width="533"
            LastChildFill="False">

            <TabControl 
                x:Name="tabPlayControls"
                Background="Transparent" 
                BorderBrush="Transparent" 
                DockPanel.Dock="Top"
                VerticalAlignment="Bottom"
                HorizontalAlignment="Stretch">
                <TabControl.Resources>
                    <Style TargetType="TabItem">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="TabItem">
                                    <Label Name="Panel">
                                        <ContentPresenter x:Name="ContentSite"
                                            VerticalAlignment="Top"
                                            HorizontalAlignment="Center"
                                            ContentSource="Header"
                                            Margin="5,0"/>
                                    </Label>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                            <Setter TargetName="Panel" Property="Foreground" Value="LightGreen" />
                                        </Trigger>
                                        <Trigger Property="IsSelected" Value="False">
                                            <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                            <Setter TargetName="Panel" Property="Foreground" Value="Black" />
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </TabControl.Resources>

                <TabItem Header="Edit Play">
                    <TabControl 
                        x:Name="tabPlay"
                        Background="Transparent" 
                        BorderBrush="Transparent" 
                        DockPanel.Dock="Bottom"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Center">
                        <TabControl.Resources>
                            <Style TargetType="TabItem">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="TabItem">
                                            <Label Name="Panel">
                                                <ContentPresenter x:Name="ContentSite"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Center"
                                                ContentSource="Header"
                                                Margin="5,0"/>
                                            </Label>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsSelected" Value="True">
                                                    <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                                    <Setter TargetName="Panel" Property="Foreground" Value="LightGreen" />
                                                </Trigger>
                                                <Trigger Property="IsSelected" Value="False">
                                                    <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                                    <Setter TargetName="Panel" Property="Foreground" Value="Black" />
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </TabControl.Resources>
                        <TabItem Header="PBPL">
                            <Grid HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="0" Grid.Row="0" Text="rec"/>
                                <TextBox IsEnabled="False" Grid.Column="0" Grid.Row="1" Text="{Binding PBPL.rec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="1" Grid.Row="0" Text="PKGE"/>
                                <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding PBPL.PKGE, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="2" Grid.Row="0" Text="COMF"/>
                                <TextBox Grid.Column="2" Grid.Row="1" Text="{Binding PBPL.COMF, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="3" Grid.Row="0" Text="pbpl"/>
                                <TextBox Grid.Column="3" Grid.Row="1" Text="{Binding PBPL.pbpl, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="4" Grid.Row="0" Text="PLYL"/>
                                <TextBox Grid.Column="4" Grid.Row="1" Text="{Binding PBPL.PLYL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="5" Grid.Row="0" Text="PBST"/>
                                <TextBox Grid.Column="5" Grid.Row="1" Text="{Binding PBPL.PBST, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="6" Grid.Row="0" Text="ord_"/>
                                <TextBox IsEnabled="False"  Grid.Column="6" Grid.Row="1" Text="{Binding PBPL.ord_, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="7" Grid.Row="0" Text="name"/>
                                <TextBox Grid.Column="7" Grid.Row="1" Text="{Binding PBPL.name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="8" Grid.Row="0" Text="Flag"/>
                                <TextBox Grid.Column="8" Grid.Row="1" Text="{Binding PBPL.Flag, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </TabItem>
                        <TabItem Header="PLYL">
                            <Grid HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="0" Grid.Row="0" Text="rec"/>
                                <TextBox IsEnabled="False" Grid.Column="0" Grid.Row="1" Text="{Binding PLYL.rec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="1" Grid.Row="0" Text="SETL"/>
                                <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding PLYL.SETL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="2" Grid.Row="0" Text="plyl"/>
                                <TextBox Grid.Column="2" Grid.Row="1" Text="{Binding PLYL.plyl, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="3" Grid.Row="0" Text="SRMM"/>
                                <TextBox Grid.Column="3" Grid.Row="1" Text="{Binding PLYL.SRMM, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="4" Grid.Row="0" Text="SITT"/>
                                <TextBox Grid.Column="4" Grid.Row="1" Text="{Binding PLYL.SITT, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="5" Grid.Row="0" Text="PLYT"/>
                                <TextBox Grid.Column="5" Grid.Row="1" Text="{Binding PLYL.PLYT, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="6" Grid.Row="0" Text="PLF_"/>
                                <TextBox Grid.Column="6" Grid.Row="1" Text="{Binding PLYL.PLF_, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="7" Grid.Row="0" Text="name"/>
                                <TextBox Grid.Column="7" Grid.Row="1" Text="{Binding PLYL.name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="8" Grid.Row="0" Text="risk"/>
                                <TextBox Grid.Column="8" Grid.Row="1" Text="{Binding PLYL.risk, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="9" Grid.Row="0" Text="motn"/>
                                <TextBox Grid.Column="9" Grid.Row="1" Text="{Binding PLYL.motn, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="10" Grid.Row="0" Text="phlp"/>
                                <TextBox Grid.Column="10" Grid.Row="1" Text="{Binding PLYL.phlp, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="11" Grid.Row="0" Text="vpos"/>
                                <TextBox Grid.Column="11" Grid.Row="1" Text="{Binding PLYL.vpos, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="plylChanged"/>
                            </Grid>
                        </TabItem>
                        <TabItem Header="PLPD">
                            <DockPanel>
                                <Grid 
                                    DockPanel.Dock="Top"
                                    HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="0" Grid.Row="0" Text="rec"/>
                                    <TextBox IsEnabled="False" Grid.Column="0" Grid.Row="1" Text="{Binding PLPD.rec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                    <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="1" Grid.Row="0" Text="PLYL"/>
                                    <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding PLPD.PLYL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                </Grid>
                                <DataGrid 
                                    DockPanel.Dock="Top"
                                    ItemsSource="{Binding PLPD.progressions, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                    HorizontalAlignment="Center"/>
                            </DockPanel>
                        </TabItem>
                        <TabItem Header="PLRD">
                            <Grid 
                                Grid.Column="0" Grid.Row="0"
                                DockPanel.Dock="Left"
                                HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="0" Grid.Row="0" Text="rec"/>
                                <TextBox IsEnabled="False" Grid.Column="0" Grid.Row="1" Text="{Binding PLRD.rec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="1" Grid.Row="0" Text="PLYL"/>
                                <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding PLRD.PLYL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="2" Grid.Row="0" Text="hole"/>
                                <TextBox Grid.Column="2" Grid.Row="1" Text="{Binding PLRD.hole, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>
                        </TabItem>
                        <TabItem Header="PBCC">
                            <DataGrid 
                                ItemsSource="{Binding PBCC, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                DockPanel.Dock="Bottom"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Center"/>
                        </TabItem>
                        <TabItem Header="PLCM">
                            <DataGrid 
                                ItemsSource="{Binding PLCM, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                DockPanel.Dock="Bottom"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Center"/>
                        </TabItem>
                        <TabItem Header="PPCT">
                            <DataGrid 
                                ItemsSource="{Binding PPCT, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                DockPanel.Dock="Bottom"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Center"/>
                        </TabItem>
                        <TabItem Header="SRFT">
                            <DataGrid 
                                ItemsSource="{Binding SRFT, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                DockPanel.Dock="Bottom"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Center"/>
                        </TabItem>
                    </TabControl>
                </TabItem>
                
                <TabItem Header="Edit Player">
                    <TabControl 
                        x:Name="tabPlayer"
                        Background="Transparent" 
                        BorderBrush="Transparent" 
                        DockPanel.Dock="Top"
                        VerticalAlignment="Bottom"
                        HorizontalAlignment="Center">
                        <TabControl.Resources>
                            <Style TargetType="TabItem">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="TabItem">
                                            <Label Name="Panel">
                                                <ContentPresenter x:Name="ContentSite"
                                                VerticalAlignment="Top"
                                                HorizontalAlignment="Center"
                                                ContentSource="Header"
                                                Margin="5,0"/>
                                            </Label>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsSelected" Value="True">
                                                    <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                                    <Setter TargetName="Panel" Property="Foreground" Value="LightGreen" />
                                                </Trigger>
                                                <Trigger Property="IsSelected" Value="False">
                                                    <Setter TargetName="Panel" Property="Background" Value="Transparent" />
                                                    <Setter TargetName="Panel" Property="Foreground" Value="Black" />
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </TabControl.Resources>
                        <TabItem Header="PLYS">
                            <Grid HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="0" Grid.Row="0" Text="rec"/>
                                <TextBox IsEnabled="False" Grid.Column="0" Grid.Row="1" Text="{Binding PLYS.rec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="1" Grid.Row="0" Text="PSAL"/>
                                <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding PLYS.PSAL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="2" Grid.Row="0" Text="ARTL"/>
                                <TextBox Grid.Column="2" Grid.Row="1" Text="{Binding PLYS.ARTL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="3" Grid.Row="0" Text="PLYL"/>
                                <TextBox Grid.Column="3" Grid.Row="1" Text="{Binding PLYS.PLYL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="4" Grid.Row="0" Text="PLRR"/>
                                <TextBox Grid.Column="4" Grid.Row="1" Text="{Binding PLYS.PLRR, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                                <TextBlock Margin="5,0" Foreground="LightGreen" Grid.Column="5" Grid.Row="0" Text="poso"/>
                                <TextBox Grid.Column="5" Grid.Row="1" Text="{Binding PLYS.poso, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" TextChanged="txtPlayerUpdated"/>
                            </Grid>
                        </TabItem>
